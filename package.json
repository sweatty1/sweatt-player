{
  "name": "sweatt-player",
  "version": "0.1.0",
  "main": "public/Main.js",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "electron-is-dev": "^1.2.0",
    "music-metadata-browser": "^2.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1"
  },
  "scripts": {
    "react-start": "cross-env BROWSER=none react-scripts start",
    "electron-start": "cross-env NODE_ENV=dev nodemon --exec \"\"electron .\"\"",
    "preelectron-pack": "npm run react-build",
    "electron-pack": "electron-builder -c.extraMetadata.main=build/Main.js",
    "reat-with-browser-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "build": {
    "appId": "nonExistentID",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "electron": "^9.0.5",
    "electron-builder": "^22.8.0",
    "electron-reload": "^1.5.0",
    "nodemon": "^2.0.4"
  }
}
